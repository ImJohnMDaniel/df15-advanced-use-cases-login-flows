    <assignments>
        <name>Change_Finish_Location_To_Sales_Manager</name>
        <label>Change Finish Location To Sales Manager</label>
        <locationX>525</locationX>
        <locationY>562</locationY>
        <assignmentItems>
            <assignToReference>LoginFlow_FinishLocation</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>/01Z50000000wdYS</stringValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Alert_Message_Sales_Are_Lower_Than_Quota_For_Current_Month</targetReference>
